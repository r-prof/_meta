#!/bin/sh

set -e

init() {
  echo init
}

sync() {
  echo sync
}

print_usage() {
  echo "Usage:"
  echo
  echo "$0 [subcommand]"
  echo
  echo "Supported subcommands:"
  echo
  echo "  init"
  echo "  sync"
}

usage() {
  print_usage 1>&2
  exit 1
}

command=$1

if [ "$command" = "" ]; then
  usage
fi

shift

case $command in
   init)
      init $@
      ;;
   sync)
      sync $@
      ;;
   *)
      usage
      ;;
esac
